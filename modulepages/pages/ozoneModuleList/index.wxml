<view class="page">
    
        <view class="v2_1" wx:for="{{deviceinfoList}}" wx:key="index" bind:tap="clickOzoneModuleDetail" data-sn="{{item.sn}}">
            <view class="v2_1_top">
                <view class="v2_1_top_1"><image class='v2_1_top_1_img' src="{{(item.Devicestatus == '空闲' ||  item.Devicestatus == '消毒中') ? '/image/icon/icon_2023_02_06_5.png' : '/image/icon/icon_2023_02_06_6.png'}}"></image>{{item.sn}}</view>
                <view class="v2_1_top_2" style="color: {{(item.Devicestatus == '空闲' ||  item.Devicestatus == '消毒中') ? '#409DFF' : '#FF2C2B'}};">{{item.Devicestatus}}</view>
            </view>
            <view class="v2_1_line"></view>
            <view class="v2_1_position"><image class='v2_1_position_img' src='/image/icon/icon_2023_01_06_25.png'></image>设备位置：{{item.address}}</view>
            <view class="v2_2">
                <view class="v2_1_time" wx:if="{{item.status}}"><image class='v2_1_position_img' src='/image/icon/icon_2023_02_06_7.png'></image>取货状态：<text style="color: {{item.status == '待审核' ? '#FF2C2B' : '#666666'}};">{{item.status}}</text></view>
                <view class="v2_2_1" wx:if="{{item.isopen == 1}}" catch:tap="clickOzoneModuleApproval" data-id="{{item.id}}">审批</view>
            </view>
            
		</view>

        <block wx:if="{{deviceinfoList.length==0}}">
                <view class="homeindex_4_1_top_2_t" style="display: flex;align-items: center;justify-content: center;padding: 100rpx;">暂无数据</view>
            </block>
    

    <view style="height:40rpx;width:0rpx"></view>
</view>