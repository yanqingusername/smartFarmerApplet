<view class="page">
      <view class="medWhite">
        <view style="height: 2rpx;"></view>

        <view class="P5 app_flex" style="margin: 14rpx 0rpx 10rpx 0rpx;">
          <view class="P5_1 app_font">照片</view>
          <view bindtap="preview" data-url="{{frontPhoto}}" style="width:480rpx;height:200rpx;margin-top:0rpx;position:relative;">
            <image src="{{frontPhoto}}" style="width:480rpx;height:200rpx;margin-top:0rpx;position:relative;" mode="aspectFill"></image>
          </view>
        </view>

        <view style="height: 2rpx;background:#DDDDDD;margin: 0rpx 20rpx;"></view>

        <view class="P5 app_flex" style="margin: 14rpx 0rpx 10rpx 0rpx;">
          <view class="P5_1 app_font">浓度</view>
          <input class="P5_2 app_font" disabled="{{true}}" placeholder="" placeholder-class="place_style" bindinput="bindName" style="position:relative;right:20rpx;margin-top: 6rpx;" value="{{name_text}}"></input>
        </view>
        <view style="height: 2rpx;background:#DDDDDD;margin: 0rpx 20rpx;"></view>

        <view class="P5 app_flex" style="margin: 14rpx 0rpx 10rpx 0rpx;">
          <view class="P5_1 app_font">有效时长</view>
          <input class="P5_2 app_font" disabled="{{true}}" placeholder="" placeholder-class="place_style" bindinput="bindTime" style="position:relative;right:20rpx;margin-top: 6rpx;" value="{{time_text}}"></input>
        </view>

        <view style="height: 2rpx;background:#DDDDDD;margin: 0rpx 20rpx;"></view>

        <view class="P5 app_flex" style="margin: 16rpx 0rpx 10rpx 0rpx;">
          <view class="P5_1 app_font">审批结果</view>
          <input class="P5_2 app_font" disabled="{{true}}" placeholder="" placeholder-class="place_style" bindinput="bindapprovaltext" style="position:relative;right:20rpx;margin-top: 6rpx;" value="{{approval_text}}"></input>
          
        </view>

        <view style="height: 10rpx;"></view>
      </view>



</view>