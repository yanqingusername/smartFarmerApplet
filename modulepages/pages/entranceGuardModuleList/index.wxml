<view class="page">

    <view class="pig_1" wx:if="{{count > 0}}">
        <view class="pig_1_1">门禁记录({{count}})</view>
    </view>
    <block wx:if="{{deviceinfoList.length > 0}}">
        <view class="v2_1" wx:for="{{deviceinfoList}}" wx:key="index">
            <view class="v2_1_top">
                <view class="v2_1_top_1"><view class="v2_1_top_1_img_view"><image class='v2_1_top_1_img' src="{{item.headerimg}}" mode="aspectFill" bindtap="preview" data-url="{{item.headerimg}}"></image></view>{{item.label_sn}}</view>
                <!-- <view class="v2_1_top_2" style="color: {{item.status == '2' ? '#FF2C2B' : '#666666'}};">{{item.status == '2' ? '离线' : '在线'}}</view> -->
            </view>
            <view class="v2_1_line"></view>
            <view class="v2_1_title" >{{item.name}}</view>
            <view class="v2_1_position">通行状态：{{item.type_desc}}</view>
            <view class="v2_1_position"><image class='v2_1_position_img' src='/image/icon/icon_2023_01_06_25.png'></image>位置：{{item.location_descr}}</view>
            <view class="v2_1_position"><image class='v2_1_position_img' src='/image/icon/icon_2023_01_06_24.png'></image>刷脸时间：{{item.create_time}}</view>
		</view>
    </block>
        <block wx:if="{{deviceinfoList.length==0}}">
            <view class="homeindex_4_1_top_2_t" style="display: flex;align-items: center;justify-content: center;padding: 100rpx;">暂无数据</view>
        </block>
    

    <view style="height:40rpx;width:0rpx"></view>
</view>